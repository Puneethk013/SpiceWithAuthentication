@model SpiceWithoutAuthentication.Identity.AppUser
@{
    ViewBag.Title = "MyProfile";
}

<h2>MyProfile</h2>
<table class="table">
    <tr>
        <th>Username</th>
        <td>@Model.Name</td>

    </tr>
    <tr>
        <th>Email</th>
        <td>@Model.Email</td>

    </tr>
    <tr>
        <th>Mobile</th>
        <td>@Model.PhoneNumber</td>

    </tr>
    <tr>
        <th>StreetAddress</th>
        <td>@Model.StreetAddress</td>

    </tr>
    <tr>
        <th>City</th>
        <td>@Model.City</td>

    </tr>
    <tr>
        <th>State</th>
        <td>@Model.State</td>

    </tr>
</table>
@Html.ActionLink("Back", "Index", "Menu")

